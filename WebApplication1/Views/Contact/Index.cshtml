
@model System.Collections.Generic.Dictionary<int,WebApplication1.Models.ContactModel>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<h2>kontakty</h2>
<a classs="btn btn-primary" asp-action="Add" asp-controller="Contact">Dodaj Kontakt</a>
<h4>Lista kontaktów</h4>
<table class="table">
<thead>
<tr>
    <th>
        imię
    </th>
    <th>
        Nazwisko
    </th>
    <th>
        Akcje
    </th>
</tr>
</thead>
<tbody>
@foreach (var entry in @Model)
{
    <tr>  
        <td>    
            @entry.Value.FirstName      
        </td>        
        <td>         
            @entry.Value.LastName    
        </td>      
        <td>         
            <div>     
                <a>Edycja</a>    
                <a>Szczegóły</a>   
                <a>Usuń</a>         
            </div>       
        </td>       
    </tr>
}    </tbody>
    </table>